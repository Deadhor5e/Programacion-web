public class Libros {
	
	private String titulo;
	private String autor;
	private  int publicacion;

	public Libros(){}
	
	public Libros(String titulo, String autor, int publicacion) {
		String libroTitulo = this.titulo;
		String libroAutor = this.autor;
		int libroPublicacion = this.publicacion;
	}
}
